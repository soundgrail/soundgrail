{
  "name": "soundgrail",
  "version": "1.0.3",
  "description": "Samples, production resources, promotion, and tips for modern musicians and music producers!",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npm run prepare && ./node_modules/mocha/bin/mocha test/ --recursive --timeout=10000",
    "prepare": "node -e \"require('prepare-package')()\"",
    "prepare:watch": "nodemon -w ./src -e '*' --exec 'npm run prepare'"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soundgrail/soundgrail"
  },
  "keywords": [
    "soundgrail",
    "music production",
    "music promotion",
    "nodejs",
    "front end"
  ],
  "author": "ITW Creative Works",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soundgrail/soundgrail/issues"
  },
  "homepage": "https://soundgrail.com",
  "preparePackage": {
    "input": "./src",
    "output": "./dist",
    "replace": {}
  },
  "dependencies": {
    "itwcw-package-analytics": "^1.0.4"
  },
  "devDependencies": {
    "mocha": "^8.4.0",
    "prepare-package": "^1.1.12"
  }
}